{
    "name": "jonwood2/guardian-php-sdk",
    "description": "A PHP SDK to work with The Guardian",
    "keywords": [
        "jonwood2",
        "guardian-php-sdk"
    ],
    "homepage": "https://github.com/jonwood2/guardian-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "jonwood2",
            "email": "jonwood2@googlemail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Jonwood2\\GuardianPhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jonwood2\\GuardianPhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
